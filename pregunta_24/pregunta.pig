lines = LOAD 'data.csv' USING PigStorage(',') AS (id,name,lastname,birthday:chararray);

result = FOREACH lines GENERATE REGEX_EXTRACT(birthday, '(.*{4})-(.*{2})-(.*{2})', 2);

STORE result INTO 'output' USING PigStorage('\t');